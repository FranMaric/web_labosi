<% let selectData = locals.selectData || { list: []};  %>
<select id="select" <%-selectData.name ? 'name="' + selectData.name + '"': '' %> <%- selectData.attributes ? selectData.attributes : '' %> >
    <% selectData.list.forEach(function(item) {  %>
        <option <%- (item.id == itemID) ? "selected" : "" %> value="<%-item.id%>"><%-item.name%></option>
    <%  }); %>
</select>
